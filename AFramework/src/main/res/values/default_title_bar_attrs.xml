<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DefaultTitlebar">
        <attr name="textTitleType" format="enum">
            <enum name="DEFAULT" value="0" />
            <enum name="DEFAULT_BOLD" value="1" />
            <enum name="SANS_SERIF" value="2" />
            <enum name="SERIF" value="3" />
            <enum name="MONOSPACE" value="4" />
        </attr>
        <attr name="titleLocation" format="enum">
            <enum name="LEFT" value="-1" />
            <enum name="CENTER" value="0" />
            <enum name="RIGHT" value="1" />
        </attr>
        <attr name="titleMarginLeft" format="dimension|reference" />
        <attr name="titleMarginRight" format="dimension|reference" />
        <attr name="textTitle" format="string|reference" />
        <attr name="textTitleColor" format="color|reference" />
        <attr name="textBack" format="string|reference" />
        <attr name="textBackColor" format="color|reference" />
        <attr name="textMenu" format="string|reference" />
        <attr name="textMenuColor" format="color|reference" />
        <attr name="imageBack" format="reference" />
        <attr name="imageMenu" format="reference" />
        <attr name="imageMenu2" format="reference" />
        <attr name="lineColor" format="color|reference" />
        <attr name="lineHeight" format="dimension|reference" />
        <attr name="layoutColor" format="color|reference" />
        <attr name="layoutHeight" format="dimension|reference" />
    </declare-styleable>

</resources>