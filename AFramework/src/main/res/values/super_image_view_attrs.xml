<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SuperImageView">
        <attr name="super_image_view_border_color" format="color" />
        <attr name="super_image_view_border_width" format="dimension" />
        <attr name="super_image_view_press_alpha" format="integer" />
        <attr name="super_image_view_press_color" format="color" />
        <attr name="super_image_view_radius" format="dimension" />
        <attr name="super_image_view_radius_top_left" format="dimension" />
        <attr name="super_image_view_radius_top_right" format="dimension" />
        <attr name="super_image_view_radius_bottom_left" format="dimension" />
        <attr name="super_image_view_radius_bottom_right" format="dimension" />
        <attr name="super_image_view_shape_type" format="enum">
            <enum name="none" value="0" />
            <enum name="round" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="ellipse" value="3" />
        </attr>
        <attr name="super_image_view_ratio" format="float" />
        <attr name="super_image_view_ratio_width" format="integer" />
        <attr name="super_image_view_ratio_height" format="integer" />
    </declare-styleable>

</resources>